<% if flash[:errors] %>
    <% flash[:errors].each do |error| %>
        <p><%= error %><p>
    <% end %>
<% end %>

<%= form_for @camper do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>
    <%= f.label :age %><br>
    <%= f.number_field :age %><br><br>
    <%= f.submit %>
<% end %>